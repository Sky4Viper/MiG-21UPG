		   <name>bomb_away 2</name>
		   <condition>
				 <property>fdm/jsbsim/systems/armament/station4/released</property>
		   </condition>
		   <path>Aircraft/MiG-21UPG/Sounds/Samples/bomb_away.wav</path>
