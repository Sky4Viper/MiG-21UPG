<?xml version="1.0"?>
 
<PropertyList>

	<fx>
	
<!-- Engine -->
 <engine>
      <name>engine-start-inside</name>
      <mode>once</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/turbine_start_in.wav</path>
      <condition>
        <and>
          <property>/controls/engines/engine[0]/starter</property>
          <property>/sim/current-view/internal</property>
        </and>
      </condition>
      <volume>
        <property>engines/engine[0]/n2</property>
        <factor> 0.01 </factor>
        <min> 0.03 </min>
        <max> 0.07 </max>
      </volume>
      <pitch>
        <property>engines/engine[0]/n2</property>
        <factor> 0.01 </factor>
        <min> 0.0 </min>
        <max> 0.9 </max>
      </pitch>
       <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
   </engine>

   
<engine>
      <name>engine-start-outside</name>
      <mode>once</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/mig21startup.wav</path>
      <condition>
        <and>
            <property>/controls/engines/engine[0]/starter</property>
            <greater-than>  
            <property>/engines/engine[0]/rpm</property>
            <value>0.5</value>
            </greater-than>
          <or>
            <not>
            <property>/sim/current-view/internal</property>
            </not>
            <greater-than>
            <property>sim/model/door-positions/canopy/position-norm</property>
            <value>0</value>
            </greater-than>
          </or>
        </and>
      </condition>
      <volume>
        <property>engines/engine[0]/n2</property>
        <factor> 0.09 </factor>
        <min> 0.3 </min>
        <max> 0.9 </max>
      </volume>
      <pitch>
        <property>engines/engine[0]/n2</property>
        <factor> 0.01 </factor>
	      <min> 0.0 </min>
	      <max> 0.9 </max>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
</engine>

<engine>
      <name>turbine</name>
      <mode>looped</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/mig21whine.wav</path>
      <condition>
        <property>/engines/engine/running</property>
        <or>
          <not>
            <property>/sim/current-view/internal</property>
          </not>
          <greater-than>
            <property>sim/model/door-positions/canopy/position-norm</property>
            <value>0</value>
          </greater-than>
        </or>
      </condition>
      <orientation>
        <x>   0 </x>
        <y>   0 </y>
        <z> 180 </z>
        <inner-cone> 180.0 </inner-cone>
        <outer-cone> 360.0 </outer-cone>
        <outer-gain>   0.2 </outer-gain>
      </orientation>
      <volume>
        <property>engines/engine/n2</property>
        <factor>0.09</factor>
        <min>0.3</min>
        <max>0.9</max>
      </volume>
      <pitch>
        <property>engines/engine/n2</property>
        <factor>0.01</factor>
	      <min>0</min>
	      <max>0.9</max>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
</engine>

 
		<jet>
			<name>jet</name>
			<path>Sounds/jet.wav</path>
			<mode>looped</mode>
			<condition>
			<equals>
			<property>engines/engine/running</property>
			<value>1</value>	
			</equals>
			</condition>
			<volume>
			<property>engines/engine[0]/thrust_lb</property>
			<factor type="double">0.00016</factor>
			</volume>
			<pitch>
			<property>engines/engine[0]/n1</property>
			<factor>0.2</factor>
			</pitch>
			<position>
			<x> 3.14 </x>
			<y>  0.0 </y>
			<z> 0.29 </z>
			</position>
			<orientation>
			<x>1.0</x>
			<y>0.0</y>
			<z>0.0</z>
			<inner-angle>100.0</inner-angle>
			<outer-angle>180.0</outer-angle>
			<outer-gain>0.2</outer-gain>
			</orientation>
		</jet>
		
		   <engine>
      <name>engine</name>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/jet-blast.wav</path>
      <mode>looped</mode>
      <condition>
        <property>engines/engine[0]/running</property>
		    <not>
        	<property>/sim/current-view/internal</property>
		      <property>engines/engine/out-of-fuel</property>
     	  </not>
      </condition>
       <position>
        <x>18.6</x>
	      <y>0</y>
	      <z>0.29</z>
      </position>
    <orientation>
	  <x>1.0</x>
    <y>0.0</y>
    <z>0</z>
    <inner-angle>35.0</inner-angle>
    <outer-angle>90</outer-angle>
    <outer-gain>0.45</outer-gain>
   </orientation>
 
   <volume>
    <property>engines/engine[0]/n1</property>
    <factor>0.014</factor>
    <offset>-0.45</offset>
   </volume>
 
   <reference-dist>20</reference-dist>
   <max-dist>3000</max-dist>
    </engine>
	
		<engine>
			<name>engine</name>
			<mode>looped</mode>
			<path>Sounds/turbine_loop.wav</path>
			<condition>
			<property>/engines/engine[0]/running</property>
			<not>
			  <property>/sim/current-view/internal</property>
			</not>
			<not>
			  <property>engines/engine[0]/out-of-fuel</property>
			</not>
			<not>
			  <property>fdm/jsbsim/systems/crash-detect/crashed</property>
			</not>
			</condition>
			<orientation>
			<x>0.0</x>
			<y>0.0</y>
			<z>180.0</z>
			<inner-cone>180.0</inner-cone>
			<outer-cone>360.0</outer-cone>
			<outer-gain>0.2</outer-gain>
			</orientation>
			<volume>
			<property>/controls/engines/engine[0]/throttle</property>
			<factor>0.5</factor>
			<min>0.1</min>
			<max>0.5</max>
			</volume>
		</engine>
	
		 <engine>
      <name>whine-int</name>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/f16-engine-int.wav</path>
      <mode>looped</mode>
      <condition>
        <property>engines/engine[0]/running</property>
        <property>/sim/current-view/internal</property>
      </condition>
       <position>
         <x> 1.16 </x>
         <y>  0.0 </y>
         <z> 0.29 </z>
       </position>
      <volume>
        <property>engines/engine[0]/n2</property>
        <factor type="double">0.003</factor>
      </volume>
      <pitch>
        <property>/engines/engine[0]/n2</property>
        <factor>0.02</factor>
        <offset>-0.1</offset>
      </pitch>
    </engine>
	
		<engine>
			<name>burner</name>
			<mode>looped</mode>
			<path>Aircraft/MiG-21UPG/Sounds/Samples/reheat.wav</path>
			<condition>
			 <property>engines/engine/afterburner</property>
        <or>
          <not>
            <property>/sim/current-view/internal</property>
          </not>
          <greater-than>
            <property>sim/model/door-positions/canopy/position-norm</property>
            <value>0</value>
          </greater-than>
        </or>
			<not>
			 <property>engines/engine[0]/out-of-fuel</property>
			</not>
			<not>
			  <property>fdm/jsbsim/systems/crash-detect/crashed</property>
			</not>
			</condition>
			<orientation>
			<x>0.0</x>
			<y>0.0</y>
			<z>180.0</z>
			<inner-cone>180.0</inner-cone>
			<outer-cone>360.0</outer-cone>
			<outer-gain>0.2</outer-gain>
			</orientation>
			<volume>
			<property>/controls/throttle[0]</property>
			<factor>1</factor>
			<min>0.4</min>
			<max>0.5</max>
			</volume>
		</engine>
		
		<engine>
			<name>burner-from-cockpit</name>
			<mode>looped</mode>
			<path>Sounds/turbojet_fan.wav</path>
			<condition>
			<property>/engines/engine[0]/augmentation</property>
			<property>/sim/current-view/internal</property>
			<not>
			<property>engines/engine[0]/out-of-fuel</property>
			</not>
			<not>
			  <property>fdm/jsbsim/systems/crash-detect/crashed</property>
			</not>
			</condition>
			<volume>
			<property>/controls/throttle[0]</property>
			<factor>0.1</factor>
			<min>0.1</min>
			<max>0.1</max>
			</volume>
		</engine>
		
		<engine>
			<name>compressor whine</name>
			<path>Sounds/turbojet_fan.wav</path>
			<mode>looped</mode>
			<condition>
        <or>
          <not>
            <property>/sim/current-view/internal</property>
          </not>
          <greater-than>
            <property>sim/model/door-positions/canopy/position-norm</property>
            <value>0</value>
          </greater-than>
        </or>
			<not>
			 <property>engines/engine[0]/out-of-fuel</property>
			</not>
			<not>
			  <property>fdm/jsbsim/systems/crash-detect/crashed</property>
			</not>
			<property>/engines/engine[0]/running</property>
			</condition>
      <position>
        <x> 1.68 </x>
        <y>  0.0 </y>
        <z> 0.29 </z>
      </position>
      <orientation>
        <!-- normalized vector -->
        <x>-1.0</x><!-- +X = aft  -->
        <y> 0.0</y><!-- +Y = left -->
        <z> 0.0</z><!-- +Z = up   -->
        <inner-angle>20.0</inner-angle>
        <outer-angle>50.0</outer-angle>
        <outer-gain>0.2</outer-gain>
      </orientation>
			<pitch>
			<property>/engines/engine[0]/n1</property>
			<offset>0.7</offset>
			<factor>.003</factor>
			</pitch>
			<volume>
			<property>/engines/engine[0]/n1</property>
			<offset>0.15</offset>
			<factor>-0.003</factor>
			<min>0.05</min>
			<max>0.15</max>
			</volume>
		</engine>
		
<!-- Gear -->
	
		<tire-rumble>
			<name>tire-rumble</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
			 <property>/sim/current-view/internal</property>
			<or>
			 <property>/gear/gear[0]/wow</property>
			 <property>/gear/gear[1]/wow</property>
			 <property>/gear/gear[2]/wow</property>
			</or>
			</condition>
			<volume>
			<property>/velocities/airspeed-kt</property>
			<factor>0.004</factor>
			<max>0.3</max>
			</volume>
		</tire-rumble>
		
		<gear-wind>
			<name>gear-wind</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
			 <property>/sim/current-view/internal</property>
			 <greater-than>
			  <property>/gear/gear[1]/position-norm[0]</property>
			  <value>0.01</value>
			 </greater-than>
			</condition>
			<volume>
			<property>/velocities/airspeed-kt</property>
			<factor>0.002</factor>
			<max>1.0</max>
			</volume>
		</gear-wind>
		
		<air-brakes>
		  <name>air-brakes</name>
		  <mode>in-transit</mode>
		  <path>Sounds/flaps.wav</path>
		  <property>surface-positions/speedbrake-pos-norm</property>
		  <volume>
			<factor>0.25</factor>
		  </volume>
		</air-brakes>
	  
	   <gear>
		  <name>gear</name>
		  <mode>in-transit</mode>
		  <path>Sounds/gear.wav</path>
		  <property>/gear/gear[0]/position-norm</property>
		  <pitch>
			<internal>dt_play</internal>
			<offset>1.0</offset>
			<factor>-0.04</factor>
		  </pitch>
		</gear>
	  
		 <flap>
		  <name>flaps</name>
		  <mode>in-transit</mode>
		  <path>Sounds/flaps.wav</path>
		  <property>/surface-positions/flap-pos-norm</property>
		  <volume>
		  <offset>0.25</offset>
		  </volume>
		</flap>
		
		<squeal>
			<name>squeal</name>
			<path>Sounds/squeal.wav</path>
			<condition>
			<or>
			 <property>/gear/gear[0]/wow</property>
			 <property>/gear/gear[1]/wow</property>
			 <property>/gear/gear[2]/wow</property>
			</or>
			</condition>
			<volume>
			<property>/velocities/speed-down-fps</property>
			<factor>0.05</factor>
			<max>1.0</max>
			</volume>
			<volume>
			<property>/velocities/airspeed-kt</property>
			<factor>0.01</factor>
			<max>1.0</max>
			</volume>
			<volume>
			<internal>dt_stop</internal>
			<factor>0.05</factor>
			<max>1.0</max>
			</volume>
			<pitch>
			<property>/velocities/airspeed-kt</property>
			<factor>0.0025</factor>
			<offset>1.2</offset>
			</pitch>
		</squeal>
		
<!-- Enviromental -->
		
		<wind>
			<name>wind</name>
			<mode>looped</mode>
			<path>Sounds/wind.wav</path>
			<condition>
			 <property>/sim/current-view/internal</property>
			</condition>
			<property>/velocities/airspeed-kt</property>
			<volume>
			<property>/fdm/jsbsim/aero/qbar-psf</property>
			<factor>0.0002</factor>
			<min>0.0</min>
			<max>0.3</max>
			</volume>
			<pitch>
			<property>/fdm/jsbsim/aero/qbar-psf</property>
			<factor>0.002</factor>
			<offset>1.25</offset>
			</pitch>
		</wind>
		
		<buffet>
			<name>alpha-buffet</name>
			<mode>looped</mode>
			<condition>
			 <property>/sim/current-view/internal</property>
			<greater-than>
			 <property>/fdm/jsbsim/aero/alpha-rad[0]</property>
			 <value>0.17</value>
			</greater-than>
			</condition>
			<path>Sounds/rumble.wav</path>
			<volume>
			<property>/fdm/jsbsim/aero/alpha-rad[0]</property>
			<factor>15.0</factor>
			<max>1.0</max>
			</volume>
			<pitch>
			<property>/fdm/jsbsim/aero/alpha-rad[0]</property>
			<factor>0.005</factor>
			</pitch>
		</buffet>

    <rain>
      <name>rain</name>
      <mode>looped</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/rain.wav</path>
      <type>avionics</type>
      <condition>
        <greater-than>
          <property>/environment/rain-norm</property>
          <value>0</value>
        </greater-than>
        <or>
          <not>
            <property>/sim/current-view/internal</property>
          </not>
          <greater-than>
            <property>sim/model/door-positions/canopy/position-norm</property>
            <value>0</value>
          </greater-than>
        </or>
      </condition>
      <volume>
        <property>velocities/airspeed-kt</property>
        <factor>0.00075</factor>
        <min>0.03</min>
        <max>0.40</max>
      </volume>
    </rain>
    
    <rain-windshield>
            <name>rain-windshield</name>
            <mode>looped</mode>
            <path>Aircraft/MiG-21UPG/Sounds/Samples/rain-windshield.wav</path>
            <type>avionics</type>
            <condition>
                <and>
                    <property>/sim/current-view/internal</property>
                    <greater-than>
                        <property>/environment/rain-norm</property>
                        <value>0</value>
                    </greater-than>
                </and>
            </condition>
            <!--<position>
                <x>-0.5</x>
                <y>0.0</y>
                <z>0.0</z>
            </position>-->
            <volume>
                <property>velocities/airspeed-kt</property>
                <factor>0.00075</factor>
                <min>0.03</min>
                <max>0.40</max>
            </volume>
        </rain-windshield>
		
<!-- Sonic boom -->
	  
		<sonicboom>
			<name>Sonic Boom</name>
			<mode>once</mode>
			<path>Aircraft/MiG-21UPG/Sounds/Samples/Sonic-Boom.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>/velocities/mach</property>
						<value>1.0</value>
					</greater-than>
					<equals>
						<property>/sim/current-view/internal</property>
						<value>false</value>
					</equals>
					<not>
						<property>/sim/crashed</property>
						<value>true</value>
					</not>
				</and>
			</condition>
		</sonicboom>
		
<!-- Stall warning -->
		
		<stall>
			<name>stall</name>
			<mode>looped</mode>
			<path>Aircraft/MiG-21UPG/Sounds/Samples/stall.wav</path>
			<condition>
			<greater-than>
				<property>orientation/alpha-deg</property>
				<value>22.5</value>
			</greater-than>
			<greater-than>
				<property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
				<value>10</value>
			</greater-than>
			<property>/sim/current-view/internal</property>
			<not>
			<property>fdm/jsbsim/systems/crash-detect/crashed</property>
			</not>
			</condition>
			<volume><factor>0.6</factor></volume>
		</stall>

    <pullup>
      <name>pullup</name>
      <mode>looped</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/pullup.wav</path>
      <condition>
        <property>/sim/current-view/internal</property>
        <property>/controls/power/sound/pulup-alert</property>
        <less-than-equals>
          <property>position/altitude-agl-ft</property>
          <value>3000</value>
        </less-than-equals>
        
        <less-than-equals>
          <property>autopilot/internal/vert-speed-fpm</property>
          <value>-4000</value>
        </less-than-equals>

        <equals>
          <property>/gear/gear[0]/position-norm</property>
          <value>0</value>
        </equals>

      </condition>
    </pullup>

    <low-fuel>
      <name>low-fuel</name>
      <mode>once</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/bingofuel.wav</path>
      <condition>
        <less-than>
          <property>/consumables/fuel/total-fuel-gals</property>
          <value>250.00</value>
        </less-than>

      </condition>
    </low-fuel>

    <low-fuel2>
      <name>low-fuel2</name>
      <mode>once</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/fuellow.wav</path>
      <condition>
        <less-than>
          <property>/consumables/fuel/total-fuel-gals</property>
          <value>150.0</value>
        </less-than>

      </condition>
    </low-fuel2>

    <low-fuel3>
      <name>low-fuel3</name>
      <mode>once</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/fuellow.wav</path>
      <condition>
        <less-than>
          <property>/consumables/fuel/total-fuel-gals</property>
          <value>100.00</value>
        </less-than>
      </condition>
    </low-fuel3>

    <low-fuel4>
      <name>low-fuel4</name>
      <mode>once</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/fuellow.wav</path>
      <condition>
        <less-than>
          <property>/consumables/fuel/total-fuel-gals</property>
          <value>50.00</value>
        </less-than>

      </condition>
    </low-fuel4>

    <low-fuel5>
      <name>low-fuel5</name>
      <mode>once</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/warning.wav</path>
      <condition>
        <less-than>
          <property>/consumables/fuel/total-fuel-gals</property>
          <value>1.0</value>
        </less-than>

      </condition>
    </low-fuel5>
		
<!-- Aircraft crash -->

		<crash>
			<name>crash</name>
			<condition>
			<property>fdm/jsbsim/systems/crash-detect/crashed</property>
			</condition>
			<path>Sounds/crash.wav</path>
		</crash>
		
<!-- Pilot-G -->
		
		<voice>
			<name>G-Force Breath Vox</name>
			<path>Aircraft/MiG-21UPG/Sounds/Samples/GForce.wav</path>
			<condition>
				<and>
					<property>/sim/current-view/internal</property>
					<greater-than>
						<property>/accelerations/pilot/z-accel-fps_sec</property>
						<value>-161</value>
					</greater-than>
				</and> 
			</condition>
			<volume>
				<factor>0.9</factor>
			</volume>
		</voice>
		
		<voice>
			<name>G-Force Vox</name>
			<path>Aircraft/MiG-21UPG/Sounds/Samples/G-Force.wav</path>
			<condition>
				<and>
					<property>/sim/current-view/internal</property>
					<less-than>
						<property>/accelerations/pilot/z-accel-fps_sec</property>
						<value>-225</value>
					</less-than>
				</and>  
			</condition>
			<volume>
				<factor>0.9</factor>
			</volume>
		</voice>
    
    <switch>
            <name>switch-click</name>
            <mode>once</mode>
            <path>Aircraft/MiG-21UPG/Sounds/Samples/switchclick.wav</path>
            <condition>
              <and>
                <property>/controls/power/sound/click-switch</property>
                <property>/sim/current-view/internal</property>
              </and>
            </condition>
            <position>
                <x>-0.3660</x>
                <y>0.3350</y>
                <z>-0.3500</z>
            </position>
            <reference-dist>0.2</reference-dist>
            <max-dist>5.0</max-dist>
      </switch>
		
<!-- Weapons -->
		
<!-- Guns fire -->

    <guns>
      <name>gunfire</name>
      <mode>looped</mode>
      <!--<path>Aircraft/MiG-21UPG/Sounds/Samples/gun.wav</path>-->
      <path>Aircraft/MiG-21UPG/Sounds/Samples/GSh23.wav</path>
      <condition>
        <property>sim/multiplay/generic/int[8]</property>
      </condition>
      <volume>
        <factor>1.0</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
      <reference-dist>600.0</reference-dist>
      <max-dist>1000.0</max-dist>
    </guns>
	
<!-- Missile fire -->

    <missile-launch1>
      <name>missile-launch1</name>
      <condition>
        <property>controls/armament/sound/launch1</property>
      </condition>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/Missile1.wav</path>
    </missile-launch1>
    
    <missile-launch2>
      <name>missile-launch2</name>
      <condition>
        <property>controls/armament/sound/launch2</property>
      </condition>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/Missile2.wav</path>
    </missile-launch2>
    
<!-- Heat Seaker  -->

    <heat-seaker>
      <name>heat-seaker</name>
      <mode>looped</mode>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/115hz-square.wav</path>
      <condition>
        <and>
          <property>/sim/current-view/internal</property>
          <property>controls/armament/masterarm</property>
          <property>instrumentation/radar/lock</property>
          <equals>
            <property>controls/armament/selectedweapon</property>
            <value>4</value>
          </equals>
          <or>
            <equals>
              <property>sim/weight[3]/selected</property>
              <value>R-60 missile</value>
            </equals>
            <equals>
              <property>sim/weight[3]/selected</property>
              <value>R-73 missile</value>
            </equals>
            <equals>
              <property>sim/weight[7]/selected</property>
              <value>R-60 missile</value>
            </equals>
            <equals>
              <property>sim/weight[7]/selected</property>
              <value>R-73 missile</value>
            </equals>
          </or>
        </and>
      </condition>
      <volume>
        <factor>0.03</factor>
        <max>0.09</max>
      </volume>
    </heat-seaker>

<!-- Rockets fire -->
	

    <UB-32>
      <name>ub-32</name>
      <condition>
        <property>sim/multiplay/generic/int[9]</property>
      </condition>
      <path>Aircraft/MiG-21UPG/Sounds/Samples/rockets_ripple.wav</path>
    </UB-32>

<!-- Flare/Chaff release -->

    <flare>
      <name>Flares</name>

      <condition>
        <property>controls/armament/trigger2</property>
      </condition>

      <path>Aircraft/MiG-21UPG/Sounds/Samples/Flare.wav</path>
    </flare>
	
<!-- Bomb release -->

    <BOMB-AWAY>
      <name>bomb-release</name>
      <!--<mode>once</mode>-->
      <path>Aircraft/MiG-21UPG/Sounds/Samples/Drop.wav</path>
      <condition>
          <property>controls/armament/sound/drop</property>
      </condition>
    </BOMB-AWAY>
    
 <!-- Chute jettison -->

    <CHUTE>
      <name>chute-deploy</name>
      <!--<mode>once</mode>-->
      <path>Aircraft/MiG-21UPG/Sounds/Samples/Chute.wav</path>
      <condition>
        <and>
        <property>controls/flight/chute_deploy</property>
        <not>
          <property>/sim/current-view/internal</property>
        </not>
        </and>
      </condition>
    </CHUTE>

	
