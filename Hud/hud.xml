<name>Tachiametric Gunsight</name>
	<enable3d>true</enable3d>

  <aiming-reticle>

    <condition>
      <and>
      <equals>
        <property>/sim/current-view/name</property>
        <value>Cockpit View</value>
      </equals>
        <or>
          <not-equals>
            <property>sim/weight[3]/selected</property>
            <value>none</value>
          </not-equals>
          <not-equals>
            <property>sim/weight[0]/selected</property>
            <value>none</value>
          </not-equals>
          <not-equals>
            <property>sim/weight[1]/selected</property>
            <value>none</value>
          </not-equals>
          <not-equals>
            <property>sim/weight[6]/selected</property>
            <value>none</value>
          </not-equals>
      <not-equals>
        <property>sim/weight[7]/selected</property>
        <value>none</value>
      </not-equals>
        </or>
      </and>
    </condition>
    
    <x>-7</x>
    <y>-1</y>
    <width>14</width>
    <!-- defines the diameter of the constant/inner circle -->
    <height>14</height>
    <!-- <active-condition> -->
    <!-- <property>/aim/active</property> -->
    <!-- </active-condition> -->
    <!-- <diameter-input> -->
    <!-- defines the diameter of the adjustable/outer circle -->
    <!-- <property>2</property> -->
    <!-- </diameter-input> -->
    
  </aiming-reticle>

