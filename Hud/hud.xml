<?xml version="1.0"?>

<PropertyList>



    <enable3d>true</enable3d>

  <tape>
    <name>Gyrocompass</name>
    <x>-25</x>
    <y>59</y>
    <width>50</width>
    <height>12</height>
    <input>
      <property>orientation/heading-deg</property>
      <factor>0.1</factor>
      <max>85</max>
      <min>0</min>
    </input>
    <display-span>3</display-span>
    <option>top</option>
    <major-divisions>1</major-divisions>
    <minor-divisions>0.5</minor-divisions>
    <tick-bottom>false</tick-bottom>
    <tick-top>false</tick-top>
    <tick-right>false</tick-right>
    <tick-left>false</tick-left>
    <cap-bottom>true</cap-bottom>
    <cap-top>false</cap-top>
    <cap-right>true</cap-right>
    <cap-left>true</cap-left>
    <marker-offset>0</marker-offset>
    <enable-pointer>false</enable-pointer>
    <pointer-type>fixed</pointer-type>
    <tick-type>line</tick-type>
    <tick-length>fixed</tick-length>
  </tape>

  <label>
    <name>Gyro Heading Marker</name>
    <x>0</x>
    <y>52</y>
    <width>1</width>
    <height>1</height>
    <format>^</format>
  </label>

  <label>
    <name>Gyro Heading Label</name>
    <x>-9</x>
    <y>68</y>
    <width>18</width>
    <height>8</height>
    <input>
      <property>/orientation/heading-deg</property>
    </input>
    <format>%4.0f</format>
    <halign>center</halign>
    <box>true</box>
  </label>

  <label>
    <name>KIAS</name>
    <x>-58</x>
    <y>60</y>
    <width>25</width>
    <height>10</height>
    <input>
      <property>/velocities/airspeed-kt</property>
    </input>
    <format>%4.0f</format>
    <halign>left</halign>
    <box>true</box>
    <option>right</option>
  </label>

  <label>
    <name>Altitude</name>
    <x>30</x>
    <y>60</y>
    <width>28</width>
    <height>10</height>
    <input>
      <property>/position/altitude-ft</property>
    </input>
    <format>%4.0f</format>
    <halign>right</halign>
    <box>true</box>
    <option>left</option>
  </label>

  <dial>
    <name>Altitude Dial</name>
    <x>45</x>
    <y>40</y>
    <width>20</width>
    <height>20</height>
    <input>
      <property>/instrumentation/altimeter/indicated-altitude-ft</property>
      <factor>1.0</factor>
      <damp>0.3</damp>
    </input>
    <format>%n</format>
    <box>false</box>
    <halign>left</halign>
    <options>4</options>
    <option>notext</option>
    <justification>0</justification>
    <blinking>0</blinking>
    <working>true</working>
    <latitude>true</latitude>
    <longitude>true</longitude>
    <tick-type>line</tick-type>
    <tick-bottom>true</tick-bottom>
    <tick-left>true</tick-left>
    <cap-bottom>true</cap-bottom>
    <display-span>0</display-span>
    <radius>15</radius>
    <divisions>10</divisions>
  </dial>

