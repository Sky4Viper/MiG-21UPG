<?xml version="1.0"?>

<PropertyList>

	<sim>
    
		<!-- Info -->
    
		<description>Mikoyan-Gurevich MiG-21 BISON (YASIM)</description>
		<author>pjedvaj, Petar Jedvaj, David Culp, Raptor, Stuart Cassie, Gary Brown, Brendan Black and Tomaskom, Skydive</author>
		<status>1.0.0</status>
		<flight-model>yasim</flight-model>
		<aero>MiG-21UPG-yasim</aero><!-- remove this line for FG versions earlier than 3.2 -->

		<rating>
			<FDM type="int">3</FDM>
			<systems type="int">1</systems>
			<cockpit type="int">3</cockpit>
			<model type="int">5</model>
		</rating>
    
		<!-- System settings -->
    
		<startup>
			<splash-texture>Aircraft/MiG-21UPG/Splash.png</splash-texture>
		</startup>
	
		<model>
			<path>Aircraft/MiG-21UPG/Models/MiG-21UPG.xml</path>
			<hide-tanks type="int">0</hide-tanks>
			<hide-lights type="int">1</hide-lights>	
		</model>

		<submodels>
		     <serviceable type="bool">true</serviceable>
         <path>Aircraft/MiG-21UPG/Submodels.xml</path>
		</submodels>
	
		<sound>
			<path>Aircraft/MiG-21UPG/Sounds/Sound.xml</path>
		</sound>
    
		<systems>
			<electrical>
				<path>Aircraft/Generic/generic-electrical.xml</path>
			</electrical>
			<autopilot>
				<path>Aircraft/MiG-21UPG/Systems/Autopilot.xml</path>
			</autopilot>
		</systems>
	
		<!--<fuel-fraction>1.0</fuel-fraction>-->
    
		<flaps>
			<setting> 0.00 </setting>
			<setting> 0.50 </setting>
			<setting> 1.00 </setting>
		</flaps>
    
		<!-- Head Up Display -->

    <hud>
      <path n="1">Aircraft/MiG-21UPG/Hud/hud.xml</path>
      <visibility n="1">true</visibility>
      <palette>
        <color n="0">
          <alpha type="float">0.85</alpha>
          <antialiased type="bool">true</antialiased>
          <brightness type="float">0.85</brightness>
          <transparent type="bool">true</transparent>
          <red type="float">0.38</red>
          <green type="float">1.0</green>
          <blue type="float">0.22</blue>
        </color>
        <color n="1">
          <alpha type="float">0.85</alpha>
          <antialiased type="bool">true</antialiased>
          <brightness type="float">0.85</brightness>
          <transparent type="bool">true</transparent>
          <red type="float">1.0</red>
          <green type="float">0.2</green>
          <blue type="float">0.0</blue>
        </color>
      </palette>
      <clipping>
        <left type="double">-60</left>
        <right type="double">60</right>
        <top type="double">85</top>
        <bottom type="double">-73</bottom>
      </clipping>
    </hud>
    
		<!-- View settings -->
    
		<virtual-cockpit archive="y">true</virtual-cockpit>
	
		<chase-distance-m type="double" archive="y">-20.0</chase-distance-m>

    <view n="0">
      <internal archive="y">true</internal>
      <config>
        <pitch-offset-deg>-12.0</pitch-offset-deg>
        <x-offset-m archive="y"> 0.00</x-offset-m>
        <y-offset-m archive="y"> 1.06</y-offset-m>
        <z-offset-m archive="y">-3.30</z-offset-m>
        <default-field-of-view-deg type="double">75</default-field-of-view-deg>
      </config>
    </view>
    
		<!-- Menu -->
    
		<menubar>
			<default>
				<menu n="100">
					<label>MiG-21UPG</label>
					<enabled type="bool">true</enabled>
					<item>
						<label>Select Livery</label>
						<binding>
							<command>nasal</command>
							<script>aircraft.livery.dialog.toggle()</script>
						</binding>
					</item>
					<item>
						<label>Reload cannon</label>
						<binding>
							<command>nasal</command>
							<script>cannon.reload()</script>
						</binding>
					</item>
					<item>
						<label>Reload rockets</label>
						<binding>
							<command>nasal</command>
							<script>rockets.reload()</script>
						</binding>
					</item>
          <item>
            <label>Reload dragchute</label>
            <binding>
              <command>nasal</command>
              <script>dragchute.reload()</script>
            </binding>
          </item>
					<item>
						<label>Toggle ammo reports</label>
						<binding>
							<command>property-toggle</command>
							<property>/controls/armament/report-ammo</property>
						</binding>
					</item>
				</menu>
			</default>
		</menubar>
    
		<!-- Help -->
	  	
		<help>
			<title>MiG-21UPG</title>
			<key>
				<name>}</name>
				<desc>Start up / Shut down engines</desc>
			</key>
      <key>
        <name>s</name>
        <desc>Toggle starter valves</desc>
      </key>
			<key>
				<name>c</name>
				<desc>Toggle canopy</desc>
			</key>
			<key>
				<name>N/n</name>
				<desc>Extend/Retract airbrake gradually</desc>
			</key>
      <key>
        <name>Ctrl-b</name>
        <desc>Toggle air brakes</desc>
      </key>
			<key>
				<name>Shift-e</name>
				<desc>Eject</desc>
			</key>
      <key>
        <name>p</name>
        <desc>Deploy drag parachute</desc>
      </key>
      <key>
        <name>j</name>
        <desc>Jettison drag parachute</desc>
      </key>
      <key>
        <name>f</name>
        <desc>Fire gun</desc>
      </key>
      <key>
        <name>d</name>
        <desc>Fire rockets</desc>
      </key>
      <key>
        <name>q</name>
        <desc>Deploy flares</desc>
      </key>
        
			<line>Parking brake is on</line>
		</help>
    
		<!-- Multiplayer -->
		
		<mp-systems>
					<radar-enabled type="bool">true</radar-enabled>
					<ecm-rwr-enabled type="bool">true</ecm-rwr-enabled>
					<combat-enabled type="bool">true</combat-enabled>
	  </mp-systems>
    <!-- Multiplayer -->
    
		<multiplay>
			<chat_display>1</chat_display>
			<generic> 
				<int n="0" alias="/sim/crashed" />
				<!-- Beacon on -->
				<int n="1" alias="/controls/lighting/beacon-switch" />
				<!-- Landing lights on -->
				<int n="2" alias="/controls/lighting/landing-lights-switch" />
				<!-- Taxi light on -->
				<int n="3" alias="/controls/lighting/taxi-light" />
				<!-- Nav lights on -->
				<int n="4" alias="/controls/lighting/nav-lights-switch" />
				<!-- Engine status -->
				<int n="5" alias="/engines/engine[0]/state" />
        <int n="6" alias="/controls/flight/chute_deploy" />
				<int n="7" alias="/sim/crashed" />
				<int n="8" alias="/controls/armament/trigger-GSh-23" />
        <int n="9" alias="/controls/armament/trigger-S-13" />
				<float n="0" alias="/surface-positions/left-taileron-pos-norm"/>
				<float n="1" alias="/surface-positions/right-taileron-pos-norm"/>
				<float n="2" alias="/surface-positions/left-flap-pos-norm"/>
				<float n="3" alias="/surface-positions/right-flap-pos-norm"/>
				<float n="4" alias="/sim/model/nozzle-animation"/>
        <float n="6" alias="/controls/flight/chute_jettisoned"/>
				<float n="8" alias="/engines/engine[0]/afterburner" /> <!-- Reheat left engine -->
			</generic>
		</multiplay>
	
