  <!-- Misc Settings -->

  <controls>
    <flight>
      <flaps>0</flaps>
      <elevator-trim>-0.16</elevator-trim>
      <chute_deploy type="bool">false</chute_deploy>
      <chute_jettisoned type="bool">false</chute_jettisoned>
      <chute_loaded type="bool">true</chute_loaded>
      <chute_extended type="bool">false</chute_extended>
    </flight>
    
    <gear>
      <brake-parking type="double">1</brake-parking>
    </gear>
    
    <lighting>
      <instruments-norm>0.3</instruments-norm>
    </lighting>
 
		<armament>
			<trigger>0</trigger>
			<trigger1>0</trigger1>
			<trigger2>0</trigger2>
			<station n="0">
