	<!-- Keyboard shortcuts -->

	<input>
		<keyboard>
      
      <key n="23">
          <name>Ctrl-w</name>
          <desc>Cycle through MasterArm modes</desc>
          <binding>
            <command>property-toggle</command>
            <property>/controls/armament/masterarm</property>
          </binding>
          <binding>
            <command>nasal</command>
            <script>switchclicksound.click("switch")</script>
          </binding>
       </key>
       
			<key n="78">
				<name>N</name>
				<desc>Extend speedbrake</desc>
				<binding>
					<command>property-adjust</command>
					<property>/controls/flight/speedbrake</property>
					<step type="double">0.2</step>
					<min type="double">0</min>
					<max type="double">1</max>
				</binding>
			</key>

      <key n="81">
        <name>Shift-Q</name>
        <desc>Fuel cut off</desc>
        <binding>
          <condition>
            <property>/sim/input/selected/engine[0]</property>
          </condition>
          <command>property-toggle</command>
          <property>/controls/engines/engine[0]/cutoff</property>
        </binding>
      </key>
       
			<key n="99">
				<name>c</name>
				<desc>Toggle canopy</desc>
				<binding>
					<command>nasal</command>
					<script>canopy.canopy_switch(2)</script>
				</binding>
			</key>

      <key n="100">
        <name>d</name>
        <desc>Select weapon</desc>
        <binding>
          <command>property-adjust</command>
          <property>controls/armament/selectedweapon</property>
          <step>1</step>
          <min>0.0</min>
          <max>5</max>
          <wrap>true</wrap>
        </binding>
        <binding>
          <command>property-adjust</command>
          <property>controls/hud/hudmode</property>
          <step>1</step>
          <min>0.0</min>
          <max>5</max>
          <wrap>true</wrap>
        </binding>
        <binding>
          <command>nasal</command>
          <script>switchclicksound.click("switch")</script>
        </binding>
      </key>
			
			<key n="101">
				<name>Shift-E</name>
				<desc>Eject</desc>
				<binding>
					<command>nasal</command>
					<script>Eject.eject();</script>
				</binding>
			</key>

      <key n="102">
        <name>f</name>
        <desc>Fire selected weapon</desc>
        <binding>
          <command>nasal</command>
          <script>switchclicksound.click("switch")</script>
        </binding>
        <binding>
          <command>property-assign</command>
          <property>controls/armament/trigger</property>
          <value>true</value>
        </binding>
        <binding>
          <command>nasal</command>
          <script>switchclicksound.click("switch")</script>
        </binding>
        <mod-up>
          <binding>
            <command>property-assign</command>
            <property>controls/armament/trigger</property>
            <value>false</value>
          </binding>
          <binding>
            <command>nasal</command>
            <script>switchclicksound.click("switch")</script>
          </binding>
        </mod-up>
      </key>

      <key n="106">
        <name>j</name>
        <desc>jettison braking parachute</desc>
        <binding>
          <command>nasal</command>
          <script>dragchute.jettison()</script>
        </binding>
      </key>
       
			<key n="110">
				<name>n</name>
				<desc>Retract speedbrake</desc>
				<binding>
					<command>property-adjust</command>
					<property>/controls/flight/speedbrake</property>
					<step type="double">-0.2</step>
					<min type="double">0</min>
					<max type="double">1</max>
				</binding>
			</key>

      <key n="112">
        <name>p</name>
        <desc>Deploy braking parachute</desc>
        <binding>
          <condition>
            <property>controls/flight/chute_loaded</property>
          </condition>
          <command>property-assign</command>
          <property>controls/flight/chute_deploy</property>
          <value type="bool">1</value>
        </binding>
        <binding>
          <condition>
            <property>controls/flight/chute_loaded</property>
          </condition>
          <command>property-assign</command>
          <property>controls/flight/chute_loaded</property>
          <value type="bool">0</value>
        </binding>
      </key>
       
			<key n="113">
				<name>q</name>
				<desc>Deploy countermeasures</desc>
				<binding>
          <condition>
            <equals>
              <property>controls/armament/ecm-prog</property>
              <value>0</value>
            </equals>
          </condition>
					<command>nasal</command>
					<script>guns.fire_FL()</script>
				</binding>
        <binding>
          <condition>
            <greater-than>
              <property>controls/armament/ecm-prog</property>
              <value>0</value>
            </greater-than>
          </condition>
          <command>property-toggle</command>
          <property>controls/armament/ecm-on</property>
        </binding>
				<mod-up>
					<binding>
            <condition>
              <equals>
                <property>controls/armament/ecm-prog</property>
                <value>0</value>
              </equals>
            </condition>
						<command>nasal</command>
						<script>guns.stop_FL()</script>
					</binding>
				</mod-up>
			</key>

      <key n="115">
        <name>s</name>
        <desc>Toggle starter valves</desc>
        <binding>
          <command>property-toggle</command>
          <property>controls/engines/engine[0]/starter</property>
        </binding>
        <mod-up>
          <binding>
            <command>null</command>
          </binding>
        </mod-up>
      </key>

      <key n="125">
        <name>}</name>
        <desc>Engine Autostart</desc>
        <binding>
          <command>nasal</command>
          <script>engines.engine1.autostart();</script>
        </binding>
      </key>

		</keyboard>
