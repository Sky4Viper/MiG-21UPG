	<!-- Keyboard shortcuts -->

	<input>
		<keyboard>
       
			<key n="78">
				<name>N</name>
				<desc>Extend speedbrake</desc>
				<binding>
					<command>property-adjust</command>
					<property>/controls/flight/speedbrake</property>
					<step type="double">0.2</step>
					<min type="double">0</min>
					<max type="double">1</max>
				</binding>
			</key>

      <key n="81">
        <name>Shift-Q</name>
        <desc>Fuel cut off</desc>
        <binding>
          <condition>
            <property>/sim/input/selected/engine[0]</property>
          </condition>
          <command>property-toggle</command>
          <property>/controls/engines/engine[0]/cutoff</property>
        </binding>
      </key>
       
			<key n="99">
				<name>c</name>
				<desc>Toggle canopy</desc>
				<binding>
					<command>nasal</command>
					<script>canopy.canopy_switch(2)</script>
				</binding>
			</key>
       
			<key n="100">
				<name>d</name>
				<desc>Fire rockets</desc>
				<binding>
					<command>nasal</command>
					<script>guns.fire_Rocket()</script>
				</binding>
				<mod-up>
					<binding>
						<command>nasal</command>
						<script>guns.stop_Rocket()</script>
					</binding>
				</mod-up>
			</key>
			
			<key n="101">
				<name>Shift-E</name>
				<desc>Eject</desc>
				<binding>
					<command>nasal</command>
					<script>Eject.eject();</script>
				</binding>
			</key>
       
			<key n="102">
				<name>f</name>
				<desc>Fire gun</desc>
				<binding>
					<command>property-assign</command>
					<property>controls/armament/trigger</property>
					<value>true</value>
				</binding>
				<mod-up>
					<binding>
						<command>property-assign</command>
						<property>controls/armament/trigger</property>
						<value>false</value>
					</binding>
				</mod-up>
			</key>

      <key n="106">
        <name>j</name>
        <desc>jettison braking parachute</desc>
        <binding>
          <command>property-assign</command>
          <property>controls/flight/chute_deploy</property>
          <value type="bool">0</value>
        </binding>
        <binding>
          <command>property-assign</command>
          <property>controls/flight/chute_jettisoned</property>
          <value type="bool">1</value>
        </binding>
        <binding>
          <command>property-assign</command>
          <property>controls/flight/chute_loaded</property>
          <value type="bool">0</value>
        </binding>
        <mod-up>
          <binding>
            <command>property-assign</command>
            <property>controls/flight/chute_jettisoned</property>
            <value type="bool">0</value>
          </binding>
        </mod-up>
      </key>
       
			<key n="110">
				<name>n</name>
				<desc>Retract speedbrake</desc>
				<binding>
					<command>property-adjust</command>
					<property>/controls/flight/speedbrake</property>
					<step type="double">-0.2</step>
					<min type="double">0</min>
					<max type="double">1</max>
				</binding>
			</key>

      <key n="112">
        <name>p</name>
        <desc>Deploy braking parachute</desc>
        <binding>
          <condition>
            <property>controls/flight/chute_loaded</property>
          </condition>
          <command>property-assign</command>
          <property>controls/flight/chute_deploy</property>
          <value type="bool">1</value>
        </binding>
      </key>
       
			<key n="113">
				<name>Ctrl-q</name>
				<desc>Deploy countermeasures</desc>
				<binding>
					<command>nasal</command>
					<script>guns.fire_FL()</script>
				</binding>
				<mod-up>
					<binding>
						<command>nasal</command>
						<script>guns.stop_FL()</script>
					</binding>
				</mod-up>
			</key>

      <key n="115">
        <name>s</name>
        <desc>Toggle starter valves</desc>
        <binding>
          <command>property-toggle</command>
          <property>controls/engines/engine[0]/starter</property>
        </binding>
        <mod-up>
          <binding>
            <command>null</command>
          </binding>
        </mod-up>
      </key>

      <key n="125">
        <name>}</name>
        <desc>Engine Autostart</desc>
        <binding>
          <command>nasal</command>
          <script>engines.engine1.autostart();</script>
        </binding>
      </key>

		</keyboard>
	</input>
    
