<?xml version="1.0"?>
<PropertyList>
	<path>MFD-2radar.ac</path>
	
	<animation>
    <type>material</type>
    <object-name>navring</object-name>
    <object-name>div</object-name>
    <object-name>rect</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor>0.8</factor>
    </emission>
    <condition>
      <property>/controls/power/battery</property>
    </condition>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>20</object-name>
    <object-name>40</object-name>
    <object-name>+</object-name>
    <object-name>-</object-name>
    <object-name>multi0</object-name>
    <object-name>multi1</object-name>
    <object-name>multi2</object-name>
    <object-name>multi3</object-name>
    <object-name>multi4</object-name>
    <object-name>multi5</object-name>
    <object-name>multi6</object-name>
    <object-name>multi7</object-name>
    <object-name>multi8</object-name>
    <object-name>multi9</object-name>
    <object-name>ai0</object-name>
    <object-name>ai1</object-name>
    <object-name>ai2</object-name>
    <object-name>ai3</object-name>
    <object-name>ai4</object-name>
    <object-name>ai5</object-name>
    <object-name>ai6</object-name>
    <object-name>ai7</object-name>
    <object-name>ai8</object-name>
    <object-name>ai9</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor>1</factor>
    </emission>
    <condition>
      <property>/controls/power/battery</property>
    </condition>
  </animation>
  
	<animation>
		<type>rotate</type>
		<object-name>navring</object-name>
		<property>orientation/heading-deg</property>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

  <!-- radar  range -->
  <animation>
    <type>select</type>
    <object-name>20</object-name>
    <condition>
      <equals>
        <property>instrumentation/radar[0]/range</property>
        <value>20</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>40</object-name>
    <condition>
      <equals>
        <property>instrumentation/radar[0]/range</property>
        <value>40</value>
      </equals>
    </condition>
  </animation>

 
