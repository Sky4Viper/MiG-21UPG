    <animation>
        <type>pick</type>
        <object-name>RMU.knob</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
	    <interval-sec>0.2</interval-sec>
            <binding>
		<condition>
			<equals>
				<property>instrumentation/rmu/unit/selected</property>
				<value>0</value>
			</equals>
		</condition>
                <command>property-adjust</command>
		<property>instrumentation/comm/frequencies/standby-mhz</property>
                <step>1</step>
		<min>118.000</min>
		<max>136.000</max>
		<wrap>1</wrap>
            </binding>
            <binding>
		<condition>
			<equals>
				<property>instrumentation/rmu/unit/selected</property>
				<value>1</value>
			</equals>
		</condition>
                <command>property-adjust</command>
		<property>instrumentation/nav/frequencies/standby-mhz</property>
                <step>1</step>
		<min>108.000</min>
		<max>118.000</max>
		<wrap>1</wrap>
            </binding>
            <binding>
		<condition>
			<equals>
				<property>instrumentation/rmu/unit/selected</property>
				<value>2</value>
			</equals>
		</condition>
                <command>property-adjust</command>
		<property>instrumentation/kt-70/inputs/digit1</property>
                <step>1</step>
		<min>0</min>
		<max>9</max>
		<wrap>1</wrap>
            </binding>
            <binding>
		<condition>
			<equals>
				<property>instrumentation/rmu/unit/selected</property>
