<?xml version="1.0"?>

<PropertyList>

 <path>gc.ac</path>

 <animation>
  <type>material</type>
  <object-name>gc.base</object-name>
  <object-name>gc.sbase</object-name>
  <object-name>gc.stick</object-name>
  <!--<emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
  </emission>-->
   <emission>
     <red>0.2</red>
     <green>0.2</green>
     <blue>0.2</blue>
     <factor-prop>controls/lighting/instruments-norm</factor-prop>
   </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>gc.stick</object-name>
  <property>controls/gear/gear-down</property>
   <interpolation>
    <entry><ind>0</ind><dep>-50</dep></entry>
    <entry><ind>1</ind><dep> 50</dep></entry>
   </interpolation>
  <center>
   <x-m> 0.006</x-m>
   <y-m>-0.006</y-m>
   <z-m> 0.000</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>gc.cz1</object-name>
  <visible>false</visible>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
    <command>property-toggle</command>
    <property>controls/gear/gear-down</property>
    </binding>
  </action>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>gc.case</object-name>
  <object-name>gc.sbase</object-name>
  <object-name>gc.stick</object-name>
  <factor-prop>/controls/lighting/panel-norm</factor-prop>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>