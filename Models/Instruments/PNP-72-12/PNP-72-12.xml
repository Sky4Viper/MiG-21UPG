<?xml version="1.0"?>

<PropertyList>

<path>PNP-72-12.ac</path>

<animation>
 <type>material</type>
 <object-name>PNP.CourseDisc</object-name>
 <object-name>PNP.BlenkerNID</object-name>
 <object-name>PNP.BlenkerNIU</object-name>
 <object-name>PNP.CN1</object-name>
 <object-name>PNP.CN2</object-name>
 <object-name>PNP.CN3</object-name>
 <object-name>PNP.CN4</object-name>
 <object-name>PNP.CN5</object-name>
 <object-name>PNP.CN6</object-name>
 <object-name>CurrentCourseMarker</object-name>
 <object-name>UpFace</object-name>
 <object-name>PNP.BlenkerK</object-name>
 <object-name>PNP.BlenkerG</object-name>
 <object-name>PNP.BlenkerKS</object-name>
 <object-name>PNP.CourseDisc</object-name>
 <object-name>PNP.IDCourse</object-name>
 <object-name>PNP.NeedleCourseAngle</object-name>
 <object-name>PNP.NCAngleRC</object-name>
 <object-name>PNP.NSetPathAngle</object-name>
 <object-name>ScaleDCGRM</object-name>
 <object-name>PlaneDCRM</object-name>
 <object-name>PlaneDGRM</object-name>
 <object-name>PNP.NCAngleRC</object-name>
 <object-name>PNP.AngleDerivIndex</object-name>
 <object-name>Face</object-name>
 <!--<emission>
  <red>0.8</red>
  <green>0.8</green>
  <blue>0.8</blue> 
 </emission>-->
  <emission>
      <red>1.1</red>
      <green>0.3</green>
      <blue>0.1</blue>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
    </emission>
</animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>pnp_glass</object-name>
  </animation>

<animation>
 <type>material</type>
 <object-name>screws</object-name>
 <emission>
  <red>0.8</red>
  <green>0.8</green>
  <blue>0.8</blue>
 </emission>
</animation>

<animation>
 <type>rotate</type>
  <object-name>PNP.CourseDisc</object-name>
  <object-name>PNP.NSetPathAngle</object-name>
  <object-name>PNP.NCAngleRC</object-name>
  <object-name>PNP.AngleDerivIndex</object-name>
  <property>orientation/heading-magnetic-deg</property>
  <factor>-1</factor>
  <center>
    <x-m> 0.0</x-m>
    <y-m> 0.0</y-m>
    <z-m>-0.00583</z-m>
  </center>
 <axis>
  <x>-1</x>
  <y>0</y>
  <z>0</z>
 </axis>
</animation>

  <!--<animation>
    <type>rotate</type>
    <object-name>PNP.NSetPathAngle</object-name>
    <property>instrumentation/heading-indicator/indicated-heading-deg</property>
    <center>
      <x-m> 0.0</x-m>
      <y-m> 0.0</y-m>
      <z-m>-0.00583</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>-->

<animation>
 <type>rotate</type>
 <object-name>PNP.NSetPathAngle</object-name>
 <property>instrumentation/nav/radials/selected-deg</property>
  <center>
    <x-m> 0.0</x-m>
    <y-m> 0.0</y-m>
    <z-m>-0.00583</z-m>
  </center>
 <axis>
  <x>-1</x>
  <y>0</y>
  <z>0</z>
 </axis>
</animation>

  
  
<animation>
  <type>rotate</type>
  <object-name>PNP.AngleDerivIndex</object-name>
  <property>autopilot/settings/heading-bug-deg</property>
  <center>
    <x-m> 0.0</x-m>
    <y-m> 0.0</y-m>
    <z-m>-0.00583</z-m>
  </center>
  <axis>
    <x>-1</x>
    <y> 0</y>
    <z> 0</z>
  </axis>
</animation>

  <!--<animation>
 <type>rotate</type>
 <object-name>PNP.NCAngleRC</object-name>
   <property>/instrumentation/gps/wp/wp[1]/bearing-mag-deg</property>
  <condition>
    <property>/instrumentation/gps/wp/wp[1]/valid</property>
  </condition>
  <center>
    <x-m> 0.0</x-m>
    <y-m> 0.0</y-m>
    <z-m>-0.00583</z-m>
  </center>
 <axis>
  <x>-1</x>
  <y> 0</y>
  <z> 0</z>
 </axis>
</animation>-->

  <animation>
    <type>rotate</type>
    <object-name>PNP.NCAngleRC</object-name>
    <property>/instrumentation/adf/indicated-bearing-deg</property>
    <min>-360</min>
    <max>360</max>
    <center>
      <x-m> 0.0</x-m>
      <y-m> 0.0</y-m>
      <z-m>-0.00583</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y> 0</y>
      <z> 0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>PNP.CN3</object-name>
    <property>instrumentation/dme/indicated-distance-nm</property>
    <factor>0.1</factor>
    <offset>-1</offset>
    <axis>
      <x> 0</x>
      <y>-1</y>
      <z> 0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>PNP.CN2</object-name>
    <property>instrumentation/dme/indicated-distance-nm</property>
    <factor>0.01</factor>
    <offset>-10</offset>
    <step>10</step>
    <scroll>0.5</scroll>
    <axis>
      <x> 0</x>
      <y>-1</y>
      <z> 0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>PNP.CN1</object-name>
    <property>instrumentation/dme/indicated-distance-nm</property>
    <factor>0.001</factor>
    <offset>-100</offset>
    <step>100</step>
    <scroll>0.5</scroll>
    <axis>
      <x> 0</x>
      <y>-1</y>
      <z> 0</z>
    </axis>
  </animation>

<animation>
 <type>textranslate</type>
 <object-name>PNP.CN4</object-name>
 <property>instrumentation/nav/radials/selected-deg</property>
 <factor>0.001</factor>
 <step>100</step>
 <axis>
  <x> 0</x>
  <y>-1</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>PNP.CN5</object-name>
 <property>instrumentation/nav/radials/selected-deg</property>
 <factor>0.01</factor>
 <step>10</step>
 <axis>
  <x> 0</x>
  <y>-1</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>PNP.CN6</object-name>
 <property>instrumentation/nav/radials/selected-deg</property>
 <factor>0.1</factor>
 <step>1</step>
 <axis>
  <x> 0</x>
  <y>-1</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>translate</type>
 <object-name>GS.Needle</object-name>
 <property>instrumentation/nav/gs-needle-deflection-norm</property>
 <factor>0.015</factor>
 <axis>
  <x>0</x>
  <y>0</y>
  <z>1</z>
 </axis>
</animation>

<animation>
 <type>translate</type>
 <object-name>ILS.HDG.Needle</object-name>
 <property>instrumentation/nav/heading-needle-deflection-norm</property>
 <factor>0.015</factor>
 <axis>
  <x>0</x>
  <y>1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>select</type>
 <object-name>PNP.BlenkerK</object-name>
 <condition>
  <equals>
   <property>mig29/o/snv</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>PNP.BlenkerG</object-name>
 <condition>
  <equals>
   <property>mig29/o/snv</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>PNP.BlenkerKS</object-name>
 <condition>
  <equals>
   <property>fdm/jsbsim/systems/IK-VK/IK-VK-on</property>
   <value>0</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>PNP.BlenkerNID</object-name>
 <condition>
   <not>
     <property>instrumentation/nav/dme-in-range</property>
   </not>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>PNP.BlenkerNIU</object-name>
 <condition>
  <equals>
   <property>mig29/o/snv</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>translate</type>
 <object-name>PNP.Button</object-name>
 <property>mig21/controls/PNP-72-12/test</property>
 <factor>0.003</factor>
 <axis>
  <x>-1</x>
  <y> 0</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>PNP.knobSetPathAngle</object-name>
 <property>instrumentation/nav/radials/selected-deg</property>
 <factor>5</factor>
 <center>
  <x-m> 0.01236</x-m>
  <y-m> 0.03020</y-m>
  <z-m>-0.03308</z-m>
 </center>
 <axis>
  <x>-1</x>
  <y> 0</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
    <type>rotate</type>
    <object-name>PNP.heading_bg</object-name>
    <property>autopilot/settings/heading-bug-deg</property>
    <factor>5</factor>
    <center>
      <x-m> 0.01236</x-m>
      <y-m>-0.02677</y-m>
      <z-m>-0.03308</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y> 0</y>
      <z> 0</z>
    </axis>
</animation>

<animation>
 <type>pick</type>
 <object-name>PNP.cz1</object-name>
 <visible>false</visible>
 <action>
  <button>0</button>
  <repeatable>false</repeatable>
  <binding>
   <command>property-assign</command>
   <property>mig21/controls/PNP-72-12/test</property>
   <value>1</value>
  </binding>
  <mod-up>
   <binding>
    <command>property-assign</command>
    <property>mig21/controls/PNP-72-12/test</property>
    <value>0</value>
   </binding>
  </mod-up>
 </action>
</animation>

<animation>
 <type>pick</type>
 <object-name>PNP.cz2</object-name>
 <visible>false</visible>
 <action>
  <button>0</button>
  <repeatable>true</repeatable>
  <binding>
   <command>property-adjust</command>
   <property>instrumentation/nav/radials/selected-deg</property>
    <step>-1</step>
    <min>1</min>
    <max>361</max>
    <wrap>true</wrap>
  </binding>
 </action>
</animation>
<animation>
 <type>pick</type>
 <object-name>PNP.cz3</object-name>
 <visible>false</visible>
 <action>
  <button>0</button>
  <repeatable>true</repeatable>
  <binding>
   <command>property-adjust</command>
   <property>instrumentation/nav/radials/selected-deg</property>
    <step>1</step>
    <min>1</min>
    <max>361</max>
    <wrap>true</wrap>
  </binding>
 </action>
</animation>
  <animation>
    <type>pick</type>
    <object-name>PNP.cz4</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>autopilot/settings/heading-bug-deg</property>
        <step>-1</step>
        <min>1</min>
        <max>361</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>PNP.cz5</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>autopilot/settings/heading-bug-deg</property>
        <step>1</step>
        <min>1</min>
        <max>361</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

</PropertyList>
