  <animation>
    <type>select</type>
    <condition>
      <property>/controls/power/radio</property>
    </condition>
    <object-name>transponder_id</object-name>
  </animation>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <condition>
      <property>/sim/rendering/rembrandt/enabled</property>
    </condition>
    <object-name>transponder_id</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>transponder_id</object-name>
    <ambient>
      <red>0.0</red>
      <green>1.0</green>
      <blue>0.0</blue>
    </ambient>
    <diffuse>
      <red>0.0</red>
      <green>0.1</green>
      <blue>0.0</blue>
    </diffuse>
    <emission>
      <red>0.3</red>
      <green>0.8</green>
      <blue>0.3</blue>
    </emission>
  </animation>

  <animation>
    <type>select</type>
    <object-name>light1</object-name>
    <condition>
      <property>controls/power/arc</property>
    </condition>
  </animation>

